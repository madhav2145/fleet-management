{
  "expo": {
    "name": "RKT Insight",
    "slug": "rkt-insight",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_logo(1).png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "This app uses notifications to alert you about important updates.",
        "NSCameraUsageDescription": "We need camera access to scan vehicle documents."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_logo(1).png",
        "backgroundColor": "#F5F5F5"
      },
      "package": "com.madhav2145.Project_Expo",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "INTERNET"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app_logo(1).png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app_logo(1).png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications"
    ],
    
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4fc8170d-505a-4ccf-8b67-5cca561b32f8"
      }
    },
    "owner": "madhav2145"
  }
}
